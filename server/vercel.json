{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/create-room", "dest": "/index.js" },
      { "src": "/join-room", "dest": "/index.js" },
      { "src": "/socket.io", "dest": "/index.js" },
      { "src": "/(.*)", "dest": "/" }
    ]
  }
  